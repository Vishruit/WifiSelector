<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="prince.wifiselector" >

    <uses-permission
        android:name="android.permission.ACCESS_WIFI_STATE"
        android:required="true" />
    <uses-permission
        android:name="android.permission.ACCESS_NETWORK_STATE"
        android:required="true" />
    <uses-permission
        android:name="android.permission.CHANGE_WIFI_STATE"
        android:required="true" />
    <uses-permission
        android:name="android.permission.INTERNET"
        android:required="true" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.STARTINGPOINT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityTwo"
            android:label="@string/title_activity_activity_two" >
        </activity>
        <activity
            android:name=".ActivityThree"
            android:label="@string/title_activity_activity_three" >
        </activity>
        <activity
            android:name=".ActivityFour"
            android:label="@string/title_activity_activity_four" >
        </activity>
        <activity
            android:name=".WifiConnection"
            android:label="@string/title_activity_wifi_connection" >
        </activity>
        <activity
            android:name=".Wifi_list"
            android:label="@string/title_activity_wifi_list" >
        </activity>
        <activity
            android:name=".MainActivity2"
            android:label="@string/title_activity_main_activity2" >
        </activity>
        <activity
            android:name=".ExpandableListView"
            android:label="@string/title_activity_expandable_list_view" >
        </activity>
        <activity
            android:name=".Scrollable"
            android:label="@string/title_activity_scrollable" >
        </activity>
        <activity
            android:name=".initialActivity"
            android:label="@string/title_activity_initial" >
        </activity>
        <activity
            android:name=".CustomListViewAndroidExample"
            android:label="@string/title_activity_expandable_list_view" >
        </activity>
        <activity
            android:name=".Calibrate"
            android:label="@string/title_activity_calibrate" >
        </activity>
        <activity
            android:name=".WirelessSwitch"
            android:label="@string/title_activity_wireless_switch" >
        </activity>
        <activity
            android:name=".objectSender"
            android:label="@string/title_activity_object_sender" >
            <intent-filter>
                <action android:name="android.intent.action.OBJECTSENDER" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".objectReceiver"
            android:label="@string/title_activity_object_receiver" >
            <intent-filter>
                <action android:name="android.intent.action.OBJECTRECEIVER" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WifiTriangulation"
            android:label="@string/title_activity_wifi_triangulation" >
        </activity>

        <receiver
            android:name=".MyReceiver"
            android:enabled="true"
            android:exported="true" >
        </receiver>

        <activity
            android:name=".sensorsData"
            android:label="@string/title_activity_sensors_data" >
        </activity>
        <activity
            android:name=".SoftwareSwitch"
            android:label="@string/title_activity_software_switch" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
